#. Cinnamenu@json->settings-schema.json->cinnamenu->description
msgid "Cinnamenu Settings"
msgstr "Cinnamenu设置"

#. Cinnamenu@json->settings-schema.json->menu-icon-custom->description
msgid "Use a custom icon"
msgstr "使用自定义图标"

#. Cinnamenu@json->settings-schema.json->menu-icon-custom->tooltip
msgid "Unchecking this allows the theme to set the icon"
msgstr "不选中此项允许主题设置图标"

#. Cinnamenu@json->settings-schema.json->apps-grid-label-width->description
#. Cinnamenu@json->settings-schema.json->apps-grid-label-width->tooltip
msgid "Applications grid label width"
msgstr "应用程序网格图标宽度"

#. Cinnamenu@json->settings-schema.json->apps-grid-label-width->units
#. Cinnamenu@json->settings-schema.json->apps-grid-icon-size->units
#. Cinnamenu@json->settings-schema.json->category-icon-size->units
#. Cinnamenu@json->settings-schema.json->apps-list-icon-size->units
msgid "pixels"
msgstr "像素"

#. Cinnamenu@json->settings-schema.json->apps-grid-icon-size->description
#. Cinnamenu@json->settings-schema.json->apps-grid-icon-size->tooltip
msgid "Applications grid icon size"
msgstr "应用程序网格图标大小"

#. Cinnamenu@json->settings-schema.json->category-icon-size->description
#. Cinnamenu@json->settings-schema.json->category-icon-size->tooltip
msgid "Category icon size"
msgstr "类别图标大小："

#. Cinnamenu@json->settings-schema.json->hover-delay->description
msgid "Menu hover delay:"
msgstr "菜单悬停延迟："

#. Cinnamenu@json->settings-schema.json->hover-delay->tooltip
msgid "Delay between switching categories"
msgstr "切换类别间的延迟"

#. Cinnamenu@json->settings-schema.json->hover-delay->units
msgid "milliseconds"
msgstr "毫秒"

#. Cinnamenu@json->settings-schema.json->menu-editor-button->description
msgid "Open the menu editor"
msgstr "打开菜单编辑器"

#. Cinnamenu@json->settings-schema.json->menu-editor-button->tooltip
msgid "Press this button to customize your menu entries."
msgstr "按此按钮自定义您的菜单条目。"

#. Cinnamenu@json->settings-schema.json->overlay-key->description
msgid "Keyboard shortcut to open and close the menu"
msgstr "打开和关闭菜单的键盘快捷键"

#. Cinnamenu@json->settings-schema.json->header0->description
msgid "Menu Entries"
msgstr "菜单条目"

#. Cinnamenu@json->settings-schema.json->header1->description
msgid "Appearance"
msgstr "外观"

#. Cinnamenu@json->settings-schema.json->apps-grid-column-count->description
#. Cinnamenu@json->settings-schema.json->apps-grid-column-count->tooltip
msgid "Number of columns in apps grid"
msgstr "应用程序网格中列的数量"

#. Cinnamenu@json->settings-schema.json->apps-grid-column-count->units
msgid "columns"
msgstr "列"

#. Cinnamenu@json->settings-schema.json->enable-bookmarks->description
msgid "Enable web bookmarks"
msgstr "启用网络书签"

#. Cinnamenu@json->settings-schema.json->enable-bookmarks->tooltip
msgid "Choose whether or not bookmarks are indexed in the menu."
msgstr "选择是否在菜单中索引书签。"

#. Cinnamenu@json->settings-schema.json->startup-view-mode->description
#. Cinnamenu@json->settings-schema.json->startup-view-mode->tooltip
msgid "Startup applications view mode"
msgstr "启动应用程序视图模式"

#. Cinnamenu@json->settings-schema.json->startup-view-mode->options
msgid "Grid"
msgstr "网格"

#. Cinnamenu@json->settings-schema.json->startup-view-mode->options
msgid "List"
msgstr "列表"

#. Cinnamenu@json->settings-schema.json->menu-label->description
#. Cinnamenu@json->settings-schema.json->menu-label->tooltip
msgid "Set the custom menu button icon name"
msgstr "设置自定义菜单按钮图标名称"

#. Cinnamenu@json->settings-schema.json->activate-on-hover->description
msgid "Open the menu when I move my mouse over it"
msgstr "当我移动鼠标到菜单上时打开菜单"

#. Cinnamenu@json->settings-schema.json->activate-on-hover->tooltip
msgid "Enable opening the menu when the mouse enters the applet"
msgstr "启用当鼠标进入小程序时打开菜单"

#. Cinnamenu@json->settings-schema.json->apps-grid-icon-scale->description
msgid "Scale grid icons by available space"
msgstr "按可用空间缩放网格图标"

#. Cinnamenu@json->settings-schema.json->apps-grid-icon-scale->tooltip
msgid ""
"This makes the grid icon size variable depending on how many apps are being "
"shown. This overrides the grid icon size option."
msgstr "这将会使网格图标大小随应用程序显示的数量而变化。这将会覆盖网格图标大小选项。"

#. Cinnamenu@json->settings-schema.json->enable-autoscroll->description
msgid "Enable autoscrolling in application list"
msgstr "在应用程序列表中启用自动滚动"

#. Cinnamenu@json->settings-schema.json->enable-autoscroll->tooltip
msgid ""
"Choose whether or not to enable smooth autoscrolling in the application list."
msgstr "选择是否在应用程序列表中启用平滑滚动。"

#. Cinnamenu@json->settings-schema.json->enable-animation->description
msgid "Use menu animations"
msgstr "使用菜单动画"

#. Cinnamenu@json->settings-schema.json->enable-animation->tooltip
msgid "Allow the menu to animate on open and close"
msgstr "允许打开和关闭菜单的动画"

#. Cinnamenu@json->settings-schema.json->menu-icon->description
msgid "Icon"
msgstr "图标"

#. Cinnamenu@json->settings-schema.json->menu-icon->tooltip
msgid "Select an icon to show in the panel."
msgstr "选择要显示在面板中的图标。"

#. Cinnamenu@json->settings-schema.json->apps-list-icon-size->description
#. Cinnamenu@json->settings-schema.json->apps-list-icon-size->tooltip
msgid "Applications list icon size"
msgstr "应用程序列表图标大小"

#. Cinnamenu@json->settings-schema.json->header2->description
msgid "Behavior"
msgstr "行为"

#. Cinnamenu@json->metadata.json->description
msgid "A flexible menu providing formatting options and bookmarks."
msgstr "一个提供格式选项和书签的灵活菜单。"

#. Cinnamenu@json->metadata.json->name
msgid "Cinnamenu"
msgstr "Cinnamenu"
